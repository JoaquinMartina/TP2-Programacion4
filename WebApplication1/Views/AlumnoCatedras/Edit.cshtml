@model WebApplication1.Models.AlumnoCatedra

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>

<h4>Inscripción</h4>
<hr />
<form asp-action="Edit">
    <div class="form-row">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group col-md-6">
            <label asp-for="AlumnoId" class="control-label"></label>
            <select asp-for="AlumnoId" class="form-control" asp-items="ViewBag.AlumnoId" disabled></select>
        </div>
        <div class="form-group col-md-6">
            <label asp-for="CatedraId" class="control-label"></label>
            <select asp-for="CatedraId" class="form-control" asp-items="ViewBag.CatedraId" disabled></select>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label asp-for="Cuatrimestre" class="control-label"></label>
            <input asp-for="Cuatrimestre" class="form-control" />
            <span asp-validation-for="Cuatrimestre" class="text-danger"></span>
        </div>
        <div class="form-group col-md-6">
            <label asp-for="FechaInscripto" class="control-label"></label>
            <input asp-for="FechaInscripto" class="form-control" />
            <span asp-validation-for="FechaInscripto" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-primary btn-lg btn-block" />
    </div>
    <div>
        <a class="btn btn-info btn-lg btn-block" asp-action="Index">Listado</a>
    </div>
</form>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="~/js/images.js"></script>